    function GetDay() {
        var ManDate = $('.ManDate').eq(row_index).val();
        var ManDay = GetManDay(ManDate);
        //alert(row_index);
        $('.ManDay').eq(row_index).val(ManDay);
    }
    function CheckReturn() {
        var borrowAmount = parseFloat($('.txtQty').eq(row_index).val());
        var returnAmount = (parseFloat($('.txtGood').eq(row_index).val()) + parseFloat($('.txtLost').eq(row_index).val()) + parseFloat($('.txtRepair').eq(row_index).val()) + parseFloat($('.txtBad').eq(row_index).val()))
        if (borrowAmount < returnAmount) {
            alert('จำนวนที่คืนจะต้องเท่ากับจำนวนที่เบิก กรุณาใส่ข้อมูลให้ถูกต้อง');
            $('.txtGood').eq(row_index).val(0);
            $('.txtLost').eq(row_index).val(0);
            $('.txtRepair').eq(row_index).val(0);
            $('.txtBad').eq(row_index).val(0);
        }
        else
        {
            Isvalidate('Update', 'JobOrderBorrow', @Request.Params["id"]);
        }
    }
</script>
