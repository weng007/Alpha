@model AlphaMvc.Models.IncomeMaster
@{
    ViewBag.Title = "Edit";
}

<h2>Master Income Edit</h2>
<script src="@Url.Content("~/Scripts/CUDS/IncomeMaster/EditIncomeMaster.js")" type="text/javascript"></script>
<div class="modal fade" id="ShowDialog" tabindex="-1" role="dialog" aria-labelledby="myModalLabel" aria-hidden="true"></div>
    @using (Html.BeginForm())
    {
        //===================================================================CSS w3school
        <form>
            <ul class="form-style">
                <li>
                    <label>Detail<span class="required">*</span></label>
                    <input type="text" id="Detail" class="field-divided" placeholder="Detail" />
                </li>
            </ul>

            <ul class="form-style">
                <li>
                    <button type="button" class="btn btn-primary" onclick="ConfirmDialog('Update', 'IncomeMaster', @Request.Params["id"])">Save</button>
                </li>
            </ul>
        </form>

    }
<script type="text/javascript">
    $(document).ready(function () {
        GetData(@Request.Params["id"]);
    });
</script>

