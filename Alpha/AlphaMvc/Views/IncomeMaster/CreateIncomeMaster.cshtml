@model AlphaMvc.Models.IncomeMaster
@{
    ViewBag.Title = "Create";
}

<h2>Master Income Create</h2>
<script src="@Url.Content("~/Scripts/CUDS/IncomeMaster/CreateIncomeMaster.js")" type="text/javascript"></script>
<div class="modal fade" id="ShowDialog" tabindex="-1" role="dialog" aria-labelledby="myModalLabel" aria-hidden="true"></div>
    <div id="container">
        @using (Html.BeginForm())
        {
            ////===================================================================CSS w3school
            <form>
                <ul class="form-style">
                    <li>
                        <label>Detail<span class="required">*</span></label>
                        <input type="text" id="txtDetail" class="field-divided text-floatleft" placeholder="" />&nbsp;<button type="button" class="btn btn-primary" onclick="Isvalidate('Create', 'IncomeMaster', 0)">Save</button>
                    </li>
                </ul>
                <br />
            </form>
        }
    </div>
<script type="text/javascript">
    $(document).ready(function () {
        CheckAuthorization();
    });
</script>
