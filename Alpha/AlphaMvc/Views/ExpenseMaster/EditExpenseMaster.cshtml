@model AlphaMvc.Models.ExpenseMaster
@{
    ViewBag.Title = "EditExpenseMaster";
}

<h2>Master Expense Edit</h2>
<script src="@Url.Content("~/Scripts/CUDS/ExpenseMaster/EditExpenseMaster.js")" type="text/javascript"></script>
<div class="modal fade" id="ShowDialog" tabindex="-1" role="dialog" aria-labelledby="myModalLabel" aria-hidden="true"></div>

    @using (Html.BeginForm())
    {
        <form>
            <ul class="form-style">
                <li>
                    <label>Detail<span class="required">*</span></label>
                    <input type="text" id="txtDetail" class="field-divided text-floatleft" placeholder="Detail" />&nbsp;<button type="button" class="btn btn-primary" onclick="Isvalidate('Update', 'ExpenseMaster', @Request.Params["id"])">Save</button>
                    
                </li>
            </ul>
            <ul class="form-style">
                <li>
                    
                </li>
            </ul>
        </form>

    }

<script type="text/javascript">

    $(document).ready(function () {

        GetData(@Request.Params["id"]);

    });

</script>