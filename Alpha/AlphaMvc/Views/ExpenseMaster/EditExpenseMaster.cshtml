@model AlphaMvc.Models.ExpenseMaster
@{
    ViewBag.Title = "EditExpenseMaster";
}

<h2>Master Expense Edit</h2>
<script src="@Url.Content("~/Scripts/CUDS/ExpenseMaster/EditExpenseMaster.js")" type="text/javascript"></script>

    @using (Html.BeginForm())
    {
        <form>
            <ul class="form-style">
                <li>
                    <label>Detail<span class="required">*</span></label>
                    <input type="text" id="Detail" class="field-divided" placeholder="Detail" />
                    
                </li>
            </ul>
            <ul class="form-style">
                <li>
                    <input type="submit" value="Save" id="Update" />
                </li>
            </ul>
        </form>

    }

<script type="text/javascript">

    $(document).ready(function () {

        GetData(@Request.Params["id"]);

    });

    $("#Update").click(function () { Update(@Request.Params["id"]); });

</script>