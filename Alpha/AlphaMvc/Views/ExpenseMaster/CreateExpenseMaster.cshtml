@model AlphaMvc.Models.ExpenseMaster
@{
    ViewBag.Title = "CreateExpenseMaster";
}

@*<h2>Master Expense Create</h2>*@
<script src="@Url.Content("~/Scripts/CUDS/ExpenseMaster/CreateExpenseMaster.js")" type="text/javascript"></script>
<!-- Modal -->
<div class="modal fade" id="ShowDialog" tabindex="-1" role="dialog" aria-labelledby="myModalLabel" aria-hidden="true"></div>

@using (Html.BeginForm())
{
    //===================================================================CSS w3school
    <form>
        <ul class="form-style">
            <li>
                <label class="label-small-22">Expense Group</label>&nbsp;<label class="label-2">Detail<span class="required">*</span></label>
                <select id="cmbTypeWorking" class="field-divided text-floatleft text-size180" required></select>&nbsp;<input type="text" id="txtDetail" class="field-divided text-size180" placeholder="" />
            </li>
            <li>
                <label>Price List<span class="required">*</span></label>
                <input type="text" id="txtPricelist" class="field-divided text-floatleft text-size120 text-right" onkeypress="return event.charCode >= 48 && event.charCode <= 57 || event.charCode == 110" value="0" onblur="convertFloat('#txtPrice')" />&nbsp;<label>Bath</label>
            </li>
            <li>
                <label>Sequence<span class="required">*</span></label>
                <input type="text" id="txtSeq" class="field-divided text-size40 text-right" onkeypress="return event.charCode >= 48 && event.charCode <= 57 || event.charCode == 110" value="1" />
            </li>
            <li>
                <button type="button" class="btn btn-primary" onclick="Isvalidate('Create', 'ExpenseMaster', 0)">Save</button>
            </li>
        </ul>
        <br />
    </form>

}
<script type="text/javascript">
    $(document).ready(function () {
        CheckAuthorization();
    });
</script>