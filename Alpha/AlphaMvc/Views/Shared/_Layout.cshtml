<!DOCTYPE html>
<html lang="en">
    <head>
        <meta charset="utf-8" />
        <title>@ViewBag.Title - My ASP.NET MVC Application</title>
        @*---------------------------- Default --------------------------*@
        <link href="@Url.Content("~/Content/Site.css")" rel="stylesheet" type="text/css" />
        <link href="@Url.Content("~/Content/Style.css")" rel="stylesheet" type="text/css" />
        <link href="@Url.Content("~/Content/jquery-ui.css")" rel="stylesheet" type="text/css" />
        <script src="@Url.Content("~/Scripts/jquery-1.12.4.js")" type="text/javascript"></script>
        @*---------------------------- ConfirmDialog --------------------------*@
        <script src="@Url.Content("~/Scripts/ConfirmDialog/ConfirmDialog.js")" type="text/javascript"></script>
        <script src="@Url.Content("~/Scripts/CUDS/UserLogin/IndexUserLogin.js")" type="text/javascript"></script>
        @*---------------------------- Menu,Table --------------------------*@
        <script src="http://ajax.googleapis.com/ajax/libs/jqueryui/1.11.2/jquery-ui.min.js"></script>
        <link href="@Url.Content("~/Content/menu.css")" rel="stylesheet" type="text/css" />
        @* ---------------------------- menu -------------------------- *@
        <link href="@Url.Content("~/Content/custom.css")" rel="stylesheet" type="text/css" />
        <script src="@Url.Content("~/Scripts/bootstrap.js")" type="text/javascript"></script>
        <script src="@Url.Content("~/Scripts/bootstrap.min.js")" type="text/javascript"></script>
        @* ---------------------------- NumberFormat -------------------------- *@
        <script src="@Url.Content("~/Scripts/NumberFormat/jquery.number.js")" type="text/javascript"></script>
        @* ---------------------------- Tab -------------------------- *@
        <link href="@Url.Content("~/Content/Tab/easy-responsive-tabs.css")" rel="stylesheet" type="text/css" />
        <script src="@Url.Content("~/Scripts/Tab/easyResponsiveTabs.js")" type="text/javascript"></script>
        @* ---------------------------- Dynamictable -------------------------- *@
        <link href="@Url.Content("~/Content/editable/addrowstyle.css")" rel="stylesheet" type="text/css" />
        <script src="@Url.Content("~/Scripts/EditableGrid/highlight.min.js")" type="text/javascript"></script>
        <script src="@Url.Content("~/Scripts/EditableGrid/jquery.dynotable.js")" type="text/javascript"></script>
        @* ---------------------------- DatePicker -------------------------- *@
        <link href="@Url.Content("~/Content/datepicker/santiago.datepicker.css")" rel="stylesheet" type="text/css" />

        <script language="javascript" type="text/javascript">
            $(document).ready(function () {
                //$('.dropdown-toggle').addClass('sf-js-enabled');
                document.getElementById("lblUserName").innerHTML = localStorage['UserName'];
            });
            
        //$(function () {
        //    $("#products").accordion();
        //    //$('#nav li:has(ul)').doubleTapToGo();
        //});
        function myFunction() {
            var x = document.getElementById("myTopnav");
            if (x.className === "topnav") {
                x.className += " responsive";
            } else {
                x.className = "topnav";
            }
        }
        </script>
        @* ---------------------------- menu -------------------------- *@
        <meta name="viewport" content="width=device-width">

    </head>
