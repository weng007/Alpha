            <div>
                @*<a href="@Url.Action("Index", "Home")" class="imagelogo"></a>*@
                @*<img src="@Url.Content("~/Images/user.png")" alt="IMAGES" class="imglogon" />*@
                <label class="logonUser">User:</label> <label id="lblUserName" class="logonname"></label>
                    @*<img src="@Url.Content("~/Images/log-in.png")" alt="IMAGES" class="imgicon" />*@
                    <div class="loginname">@Html.ActionLink("Log Out", "IndexLogin", "Login")</div>
                    @*<div class="table-responsive">*@
                    <div class="menulocation">
                        <table border="0" align="center" cellpadding="0" cellspacing="0">
                            <tr>
                                <td>
                                    <nav class="navbar navbar-default">
                                        @*<div class="container-fluid">*@
                                        <ul class="nav navbar-nav">
                                            <li>
                                                <div id="mnDashboard" class="menugray dropdown-toggle disabled" data-toggle="dropdown">@Html.ActionLink("Dashboard", "DashBoard", "Home")</div>
                                            </li>
                                            <li class="dropdown">
                                                <div id="mnAdministrator" class="menublue dropdown-toggle" data-toggle="dropdown">Administration<span class="caret"></span></div>
                                                <ul class="dropdown-menulightblue">
                                                    <li id="subIncome">
                                                        @Html.ActionLink("Income", "IndexIncomeMaster", "IncomeMaster")
                                                    </li>
                                                    <li id="subExpense">
                                                        @Html.ActionLink("Expense", "IndexExpenseMaster", "ExpenseMaster")
                                                    </li>
                                                    <li id="subSecurity">
                                                        @Html.ActionLink("SecurityProfile", "IndexSecurityProfile", "SecurityProfile")
                                                    </li>
                                                    <li id="subUser">
                                                        @Html.ActionLink("User", "IndexUser", "User")
                                                    </li>
                                                </ul>
                                            </li>
                                            <li class="dropdown">
                                                <div id="mnActivity" class="menugray dropdown-toggle" data-toggle="dropdown">Activity<span class="caret"></span></div>
                                                <ul class="dropdown-menu">
                                                    <li id="subActivity">
                                                        @Html.ActionLink("All Activity", "AllActivity", "AllActivity")
                                                    </li>
                                                    <li id="subJobCalendar">
                                                        @Html.ActionLink("Calendar Job", "IndexCalendarJob", "CalendarJob")
                                                    </li>
                                                    <li id="subManCalendar">
                                                        @Html.ActionLink("Calendar ManPower", "IndexCMP", "CalendarManPower")
                                                    </li>
                                                </ul>
                                            </li>
                                            <li class="dropdown">
                                                <div id="mnTechnician" class="menunevyblue dropdown-toggle" data-toggle="dropdown">Technician & Card<span class="caret"></span></div>
                                                <ul class="dropdown-menulightnavyblue">
                                                    <li id="subTechnician">
                                                        @Html.ActionLink("Technician", "IndexTechnician", "Technician")
