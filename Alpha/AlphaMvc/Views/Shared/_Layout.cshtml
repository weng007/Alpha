                                                    </li>
                                                    <li id="subJobOrder">
                                                        @Html.ActionLink("Job Order", "IndexJobOrder", "JobOrder")
                                                    </li>
                                                </ul>
                                            </li>
                                            <li class="dropdown">
                                                <div id="mnPayment" class="menugray dropdown-toggle" data-toggle="dropdown">Payment<span class="caret"></span></div>
                                                <ul class="dropdown-menu">
                                                    <li id="subPayment">
                                                        @Html.ActionLink("Alpha & Outsource", "IndexWageTeachnician", "WageTeachnician")
                                                    </li>
                                                </ul>
                                            </li>
                                            <li class="dropdown">
                                                <div id="mnReport" class="menupink dropdown-toggle" data-toggle="dropdown">Report<span class="caret"></span></div>
                                                <ul class="dropdown-menulightpink">
                                                    <li id="subRptJob">
                                                        @Html.ActionLink("Job Order", "IndexReportJobOrder", "ReportJobOrder")
                                                    </li>
                                                    <li id="subRptRequisition">
                                                        @Html.ActionLink("Requisition", "IndexRptBorrow", "ReportBorrow")
                                                    </li>
                                                    @*<li id="subRptJobPayment">
                                                @Html.ActionLink("Job Payment", "IndexRptJobPayment", "ReportJobPayment")
                                            </li>*@
                                                </ul>
                                            </li>
                                        </ul>
                                        @*</div>*@
                                    </nav>
                                </td>
                            </tr>
                        </table>
                    </div>
                    @*</div>*@
</div>
