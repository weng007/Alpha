                                                <div id="mnTools" class="menugray dropdown-toggle" data-toggle="dropdown">Tools & Machines<span class="caret"></span></div>
                                                <ul class="dropdown-menu">
                                                    <li id="subProducts">
                                                        @Html.ActionLink("Stock", "IndexProducts", "Products")
